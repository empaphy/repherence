    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "Empaphy\\Repherence\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    }
}
